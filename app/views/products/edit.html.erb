<h1>Edit Product</h1>
<%= form_for(@product) do |f| %>
  <p>
    <%= f.text_field :name %>  
  </p>
  <p>
    <%= f.number_field :price %>
  </p>
  <article> 
    <h4>Select Category</h4>
    <%= f.collection_check_boxes(:category_ids, Category.all, :id, :name) %>
  </article>
  <p><%= f.submit %></p>
<% end %>
